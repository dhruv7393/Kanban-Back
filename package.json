{
  "name": "kanban-backend",
  "version": "1.0.0",
  "description": "Backend API for Kanban Dashboard",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "serve": "node dist/index.js",
    "postbuild": "chmod +x bin/postbuild.sh && ./bin/postbuild.sh",
    "dev": "nodemon --watch src --ext ts,js,json --exec ts-node src/index.ts"
  },
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "mongoose": "^8.19.1",
    "typescript": "^5.3.3",
    "winston": "^3.18.3",
    "winston-cloudwatch": "^6.3.0"
  },
  "devDependencies": {
    "@types/winston": "^2.4.4",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2"
  },
  "keywords": [
    "kanban",
    "express",
    "mongodb",
    "typescript",
    "api"
