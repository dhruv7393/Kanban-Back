{
  "name": "kanban-backend",
  "version": "1.0.0",
  "description": "Backend API for Kanban Dashboard",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && npm run create-manifest",
    "create-manifest": "echo '{\"version\":1,\"framework\":\"node\",\"distDir\":\"dist\",\"buildCommand\":\"npm run build\",\"startCommand\":\"npm start\",\"routes\":[{\"route\":\"/*\",\"target\":\"/index.js\"}]}' > dist/deploy-manifest.json",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-validator": "^2.20.33",
    "@types/node": "^20.10.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "mongoose": "^8.19.1",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "tsx": "^4.6.2"
  },
  "keywords": [
    "kanban",
    "express",
    "mongodb",
    "typescript",
    "api"
  ],
  "author": "",
  "license": "MIT"
}
